{
  "cli": {
    "version": ">= 12.6.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "API_URL": "API_URL"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "API_URL": "API_URL"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "API_URL": "API_URL"
      },
      "android": {
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}